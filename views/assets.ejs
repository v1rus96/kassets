<% for(var i=0; i<images.length; i++) { %>
<div class=" col-12 col-md-3 col-lg-2 item <%= images[i].category %>">
    <% if(images[i].category != "sound") { %>
    <div class="card ">
        <input type="text" value="<%= images[i].url %>">
        <img src="<%= images[i].url %>" class="card-img-top" alt="...">
        <div class="banner-<%= images[i].category %>"><%= images[i].name %></div>

    </div>
    <% } else {%>
    <div class="card">
        <input type="text"
            value="<audio controls preload='metadata'><source src='<%= images[i].url %>'></audio>">
        <audio controls preload="metadata" class="center">
            <source src="<%= images[i].url %>">
        </audio>
        <div class="copy-button ">copy tag</div>
        <div class="banner-<%= images[i].category %>">
            <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24" fill="none"
                stroke="currentColor" stroke-width="2" stroke-linecap="round"
                stroke-linejoin="round" class="feather feather-music">
                <path d="M9 18V5l12-2v13"></path>
                <circle cx="6" cy="18" r="3"></circle>
                <circle cx="18" cy="16" r="3"></circle>
            </svg>
            <span><%= images[i].name %></span>
        </div>
    </div>
    <% } %>
</div>
<% } %>